{
  "request": {
    "method": "GET",
    "urlPath": "/v2/flights/departures",
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "equalTo": "secret1234"
      }
    },
    "queryParameters": {
      "from": {
        "matches": "^\\d{4}-\\d{2}-\\d{2}.*$"
      },
      "to": {
        "matches": "^\\d{4}-\\d{2}-\\d{2}.*$"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "destinations_response.json"
  }
}
